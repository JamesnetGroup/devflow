<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<SolidColorBrush Color="#FF222F46" x:Key="BRUSH.BG"/>
	<SolidColorBrush Color="#FFAAAAAA" x:Key="BRUSH.BG.COLORSHADOW"/>
	<SolidColorBrush Color="#DDFFFFFF" x:Key="BRUSH.BG.QUICK"/>
	<SolidColorBrush Color="#FFFFFFFF" x:Key="BRUSH.BG.QUICK.OVER"/>
    <SolidColorBrush Color="#77000000" x:Key="BRUSH.BG.SHADOW"/>

    <SolidColorBrush Color="#FF333333" x:Key="BRUSH.BD.WIN.I"/>
    <SolidColorBrush Color="#FF777777" x:Key="BRUSH.BD.WIN.O"/>

    <SolidColorBrush Color="#FF000000" x:Key="BRUSH.00"/>
    <SolidColorBrush Color="#FF333333" x:Key="BRUSH.33"/>
    <SolidColorBrush Color="#FF666666" x:Key="BRUSH.66"/>
    <SolidColorBrush Color="#FF999999" x:Key="BRUSH.99"/>
    <SolidColorBrush Color="#FFAAAAAA" x:Key="BRUSH.AA"/>
    <SolidColorBrush Color="#FFCCCCCC" x:Key="BRUSH.CC"/>
    <SolidColorBrush Color="#FFF1F1F1" x:Key="BRUSH.F1"/>
    <SolidColorBrush Color="#FFF7F7F7" x:Key="BRUSH.F7"/>
    <SolidColorBrush Color="#FFFAFAFA" x:Key="BRUSH.FA"/>
	<SolidColorBrush Color="#FFFFFFFF" x:Key="BRUSH.FF"/>

	<SolidColorBrush Color="#FFC9D1D3" x:Key="BRUSH.FONT.H01"/>
	<SolidColorBrush Color="#FFFFFFFF" x:Key="BRUSH.FONT.I01"/>

    <SolidColorBrush Color="#FF427CB3" x:Key="BRUSH.CHECK.CIRCLE"/>
	<SolidColorBrush Color="#FF666666" x:Key="BRUSH.CHECK.CIRCLE.FALSE"/>
    <SolidColorBrush Color="#FF427CB3" x:Key="BRUSH.SELECTED.LINE"/>

	<SolidColorBrush Color="#FFFFFFFF" x:Key="BRUSH.SLOT.FILL"/>
	<SolidColorBrush Color="#FF000000" x:Key="BRUSH.SLOT.BACK"/>

    <SolidColorBrush Color="#FFA370CB" x:Key="BRUSH.BG.SELECT"/>
    <SolidColorBrush Color="#FFFFFFFF" x:Key="BRUSH.FG.SELECT"/>

	<SolidColorBrush Color="#FF404B5D" x:Key="BRUSH.BD.BLINE"/>

	<SolidColorBrush Color="#CCFFFFFF" x:Key="BRUSH.COLORSL.POINT"/>

	<Storyboard x:Key="SB.WIN.IN">
		<DoubleAnimation Storyboard.TargetName="film" Storyboard.TargetProperty="(Rectangle.Opacity)" To="1" Duration="0:0:0.3" BeginTime="0" AutoReverse="False"/>
    </Storyboard>

    <Storyboard x:Key="SB.WIN.OUT">
        <DoubleAnimation Storyboard.TargetName="film" Storyboard.TargetProperty="(Rectangle.Opacity)" To="0" Duration="0:0:0.1" BeginTime="0" AutoReverse="False"/>
    </Storyboard>
</ResourceDictionary>

