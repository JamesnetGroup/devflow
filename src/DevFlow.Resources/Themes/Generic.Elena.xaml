<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<SolidColorBrush Color="#FFF7E2E2" x:Key="BRUSH.BG"/>
	<SolidColorBrush Color="#FFAAAAAA" x:Key="BRUSH.BG.COLORSHADOW"/>
	<SolidColorBrush Color="#AA333333" x:Key="BRUSH.BG.QUICK"/>
	<SolidColorBrush Color="#BB000000" x:Key="BRUSH.BG.SHADOW"/>

	<SolidColorBrush Color="#FF333333" x:Key="BRUSH.BD.WIN.I"/>
	<SolidColorBrush Color="#FF777777" x:Key="BRUSH.BD.WIN.O"/>

	<SolidColorBrush Color="#FFFFFF" x:Key="BRUSH.00"/>
    <SolidColorBrush Color="#CCCCCC" x:Key="BRUSH.33"/>
    <SolidColorBrush Color="#999999" x:Key="BRUSH.66"/>
    <SolidColorBrush Color="#999999" x:Key="BRUSH.99"/>
    <SolidColorBrush Color="#555555" x:Key="BRUSH.AA"/>
    <SolidColorBrush Color="#333333" x:Key="BRUSH.CC"/>
    <SolidColorBrush Color="#0A0A0A" x:Key="BRUSH.F1"/>
    <SolidColorBrush Color="#080808" x:Key="BRUSH.F7"/>
    <SolidColorBrush Color="#010101" x:Key="BRUSH.FA"/>
	<SolidColorBrush Color="#000000" x:Key="BRUSH.FF"/>

	<SolidColorBrush Color="#39332E" x:Key="BRUSH.FONT.H01"/>
	<SolidColorBrush Color="#000000" x:Key="BRUSH.FONT.I01"/>

    <SolidColorBrush Color="#CA6E6E" x:Key="BRUSH.CHECK.CIRCLE"/>
	<SolidColorBrush Color="#BBBBBB" x:Key="BRUSH.CHECK.CIRCLE.FALSE"/>
    <SolidColorBrush Color="#CA6E6E" x:Key="BRUSH.SELECTED.LINE"/>

	<SolidColorBrush Color="#000000" x:Key="BRUSH.SLOT.FILL"/>
    <SolidColorBrush Color="#FFFFFF" x:Key="BRUSH.SLOT.BACK"/>

    <SolidColorBrush Color="#a370cb" x:Key="BRUSH.BG.SELECT"/>
    <SolidColorBrush Color="#FFFFFF" x:Key="BRUSH.FG.SELECT"/>

    <SolidColorBrush Color="#ecbebe" x:Key="BRUSH.BD.BLINE"/>

    <Storyboard x:Key="SB.WIN.IN">
		<DoubleAnimation Storyboard.TargetName="film" Storyboard.TargetProperty="(Rectangle.Opacity)" To="1" Duration="0:0:0.3" BeginTime="0" AutoReverse="False"/>
	</Storyboard>

	<Storyboard x:Key="SB.WIN.OUT">
		<DoubleAnimation Storyboard.TargetName="film" Storyboard.TargetProperty="(Rectangle.Opacity)" To="0" Duration="0:0:0.1" BeginTime="0" AutoReverse="False"/>
	</Storyboard>
</ResourceDictionary>

