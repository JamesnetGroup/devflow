<Window x:Class="DevFlow.Main.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" WindowStyle="None"
        xmlns:vm="clr-namespace:DevFlow.Main.ViewModels"
        xmlns:quickbar="clr-namespace:DevFlow.QuickBar.Views;assembly=DevFlow.QuickBar"
        SnapsToDevicePixels="True"
        UseLayoutRounding="True"
                    xmlns:history="clr-namespace:DevFlow.Wallpaper.Views;assembly=DevFlow.History"
        Style="{StaticResource WIN}"
        Title="MainWindow">
    <Window.DataContext>
        <vm:MainViewModel/>
    </Window.DataContext>
    <Grid>
        <history:HistoryView VerticalAlignment="Center" HorizontalAlignment="Center"/>
    </Grid>
</Window>
