	xmlns:system="clr-namespace:System;assembly=mscorlib">
	<system:String x:Key="CHINA">中国</system:String>
	<system:String x:Key="FRANCE">法国</system:String>
	<system:String x:Key="GERMANY">德国</system:String>
	<system:String x:Key="JAPAN">日本</system:String>
	<system:String x:Key="KOREA">大韩民国</system:String>
